source http://www.nuget.org/api/v2/
storage: none
lowest_matching: true
nuget Microsoft.CodeAnalysis.CSharp.Workspaces >= 2 strategy: min

group Analyzers
  storage: none
  source http://www.nuget.org/api/v2/
  nuget AsyncUsageAnalyzers
  nuget IDisposableAnalyzers  
  nuget StyleCop.Analyzers
  nuget Gu.Analyzers

group Test
  storage: none
  source http://www.nuget.org/api/v2/
  nuget Microsoft.CodeAnalysis.CSharp.Workspaces ~> 2
  nuget Gu.Roslyn.Asserts ~> 2
  nuget NUnit